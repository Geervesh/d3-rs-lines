{
  "name": "@redsift/d3-rs-lines",
  "version": "0.0.1",
  "description": "Generates line charts using D3v4.",
  "keywords": [
    "line",
    "redsift",
    "d3",
    "chart"
  ],
  "homepage": "https://github.com/redsift/d3-rs-lines",
  "license": "MIT",
  "author": {
    "name": "Rahul Powar",
    "url": "https://medium.com/@rahulpowar"
  },
  "main": "distribution/d3-rs-lines.umd-es2015.js",
  "browser": "distribution/d3-rs-lines.umd-es2015.min.js",
  "jsnext:main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/redsift/d3-rs-lines.git"
  },
  "scripts": {
    "serve": "gulp -o d3-rs-lines -g d3-selection -g d3-shape -g d3-array -g d3-scale -g d3-axis serve",
    "build": "gulp -o d3-rs-lines -g d3-selection -g d3-shape -g d3-array -g d3-scale -g d3-axis build",
    "pretest": "npm outdated && npm run build",
    "test": "tape 'test/**/*-test.js' && eslint index.js src",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "d3-selection": "^0.9.0",
    "d3-shape": "^0.7.1",
    "d3-array": "^1.0.0",
    "d3-scale": "^0.9.2",
    "d3-axis": "^0.4.1",
    "d3-format": "git://github.com/d3/d3-format",
    "d3-time-format": "git://github.com/d3/d3-time-format",
    "d3-time": "^1.0.0",
    "@redsift/d3-rs-svg": "~0.2.3",
    "@redsift/d3-rs-theme": "~0.0.2",
    "@redsift/d3-rs-tip": "~0.2.3",     
    "@redsift/d3-rs-intl": "~0.0.1"
  },
  "devDependencies": {
    "@redsift/tape-reel": "~0.0.2",
    "browser-sync": "^2.13.0",
    "del": "^2.2.1",
    "eslint": "^2.13.1",
    "gulp": "^3.9.1",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^2.0.0-alpha",
    "gulp-uglify": "^1.5.3",
    "gulp-util": "^3.0.7",
    "json2module": "~0.0.3",
    "rollup": "^0.32.0",
    "rollup-plugin-node-resolve": "^1.5.0",  
    "rollup-plugin-commonjs": "^3.0.0",     
    "rollup-plugin-buble": "^0.11.0",
    "rollup-plugin-json": "^2.0.0",
    "rollup-stream": "^1.8.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "yargs": "^4.7.1"
  }
}
